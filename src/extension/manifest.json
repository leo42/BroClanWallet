{
  "manifest_version": 3,
  "name": "Bro Clan",
  "version": "0.1",
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "background": {
    "service_worker": "worker.js"
  },
  "action":{
    "default_title": "Bro Clan"
},
  "host_permissions": ["<all_urls>"]
}
